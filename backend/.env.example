COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REQUIRED_GROUP=Patients

# DynamoDB
DDB_TABLE_PATIENTS=medmitra_patients

# Optional: shared-secret for kiosk terminals
KIOSK_SHARED_SECRET=some-long-random-string
